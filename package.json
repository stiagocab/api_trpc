{
  "name": "trpc-api-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "main": "./trpc-api-export/dist/index.js",
  "types": "./trpc-api-export/dist/index.d.ts",
  "files": [
    "trpc-api-export/dist",
    "README.md"
  ],
  "scripts": {
    "trpc-api-export": "tsup --config trpc-api-export/builder/tsup.config.ts && npm run format-fix",
    "prepare": "husky install",
    "build": "shx rm -rf build && tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "semantic-release": "semantic-release --branches main"
  },
  "dependencies": {
    "@trpc/server": "10.44.1",
    "amazon-dax-client": "^1.2.9",
    "aws-sdk": "^2.1516.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "helmet": "^7.1.0",
    "response-time": "^2.3.2",
    "superjson": "2.2.1",
    "swagger-ui-express": "^5.0.0",
    "trpc-openapi": "^1.2.0",
    "validator": "^13.11.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.4",
    "@types/amazon-dax-client": "^1.2.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/response-time": "^2.3.8",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "semantic-release": "22.0.10",
    "shx": "0.3.4",
    "tsup": "8.0.1",
    "tsx": "4.6.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
